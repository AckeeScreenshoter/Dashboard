{
    "name": "ass",
    "version": "0.1.0",
    "private": true,
    "devDependencies": {
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-compat": "^3.12.0",
        "husky": "3.x",
        "imagemin-lint-staged": "0.x",
        "lint-staged": "9.x",
        "prettier": "^2.3.2",
        "prettier-config-ackee": "^0.0.15"
    },
    "dependencies": {
        "@ackee/antonio-utils": "^4.0.0-beta.0",
        "@ackee/chris": "2.x",
        "@ackee/jerome": "^4.1.0",
        "@ackee/lucas": "^0.3.1",
        "@ackee/redux-utils": "3.1.3",
        "@ant-design/icons": "^4.6.2",
        "@sentry/browser": "^6.2.2",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "antd": "^4.13.1",
        "connected-react-router": "^6.9.1",
        "date-fns": "^2.22.1",
        "fela": "^11.6.0",
        "fela-monolithic": "^11.6.0",
        "fela-plugin-friendly-pseudo-class": "^11.6.0",
        "fela-plugin-named-keys": "^11.6.0",
        "fela-plugin-unit": "^11.6.0",
        "fela-plugin-validator": "^11.6.0",
        "firebase": "^8.3.0",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-fela": "^11.6.0",
        "react-intl": "^5.13.2",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "redux": "^4.0.5",
        "redux-saga": "^1.1.3",
        "redux-sentry-middleware": "^0.2.2",
        "reselect": "^4.0.0",
        "web-vitals": "^1.0.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,json,scss,less,css,md}": [
            "yarn format",
            "git add"
        ],
        "**/*.{png,jpeg,jpg,gif,svg}": [
            "imagemin-lint-staged",
            "git add"
        ]
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "format": "prettier --config ./prettier.config.js --write ./src/**/*.{js,jsx}"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
